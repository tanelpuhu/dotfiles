#!/bin/bash
[ -z "$PS1" ] && return

for file in ~/.{extra,paths,aliases,functions,exports}; do
  [ -r "$file" ] && . "$file"
done

for file in /etc/bash_completion \
            ~/bin/.bash_completion \
            ~/bin/.bash_completion_generated \
            ~/.git-completion.bash \
            /usr/local/etc/bash_completion \
; do
  [ -r "$file" ] && . "$file"
done

unset file
